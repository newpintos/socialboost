{
  "name": "socialboost-saas",
  "version": "1.0.0",
  "private": true,
  "description": "Production-ready SaaS boilerplate for AI-powered social media content generation",
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "nanoid": "^5.1.6",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
