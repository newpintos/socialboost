# Supabase Configuration
SUPABASE_URL=http://localhost:54321
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_ANON_KEY=your-anon-key-here

# AI Provider Configuration
# Options: 'nanobanana' or 'gemini'
AI_PROVIDER=nanobanana

# NanoBanana API (Default Provider)
# Optional: If missing, uses mock adapter for local development
NANO_BANANA_API_URL=https://api.nanobanana.ai
NANO_BANANA_API_KEY=your-nanobanana-api-key

# Gemini API (Alternative Provider)
# Optional: If missing, uses mock adapter when AI_PROVIDER=gemini
GEMINI_API_KEY=your-gemini-api-key

# Payment Provider Configuration (Optional for Local Dev)
# Stripe
STRIPE_SECRET=sk_test_your_stripe_secret_key

# Razorpay
RAZORPAY_KEY_ID=rzp_test_your_key_id
RAZORPAY_KEY_SECRET=your_razorpay_secret
